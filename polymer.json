{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/sc-dashboard.html",
    "src/sc-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "addServiceWorker": false,
      "preset": "es5-unbundled"
    }
  ]
}
